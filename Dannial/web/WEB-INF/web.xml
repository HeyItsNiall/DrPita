<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>CustomerController</servlet-name>
        <servlet-class>Controller.CustomerController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RegisterServlet</servlet-name>
        <servlet-class>Controller.RegisterServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>Controller.LoginServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LogoutServlet</servlet-name>
        <servlet-class>Controller.LogoutServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DeleteBooking</servlet-name>
        <servlet-class>Controller.DeleteBooking</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EditBooking</servlet-name>
        <servlet-class>Controller.EditBooking</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EditBooking2</servlet-name>
        <servlet-class>Controller.EditBooking2</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SaveBooking</servlet-name>
        <servlet-class>Controller.SaveBooking</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ViewBooking</servlet-name>
        <servlet-class>Controller.ViewBooking</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CustomerController</servlet-name>
        <url-pattern>/CustomerController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RegisterServlet</servlet-name>
        <url-pattern>/RegisterServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/LoginServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LogoutServlet</servlet-name>
        <url-pattern>/LogoutServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeleteBooking</servlet-name>
        <url-pattern>/DeleteBooking</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EditBooking</servlet-name>
        <url-pattern>/EditBooking</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EditBooking2</servlet-name>
        <url-pattern>/EditBooking2</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SaveBooking</servlet-name>
        <url-pattern>/SaveBooking</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ViewBooking</servlet-name>
        <url-pattern>/ViewBooking</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
